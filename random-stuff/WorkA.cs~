using System;

namespace WorkA

{
  class Program
  {
    public static void Main()
    {
    //Apresentar todos os números divisíveis por 4 que sejam menores que 40. Iniciar a contagem com o valor 1 (um).
        for (int i = 1; i <= 10; i++)
        {
            Console.WriteLine(i * 4);
        }
    }
  }
}
